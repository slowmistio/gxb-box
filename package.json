{
  "name": "gxb-box",
  "version": "1.0.1",
  "description": "gxchain box",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm install pm2 -g",
    "start":"npm run gui:dev",
    "dev":"npm run gui:dev",
    "gui:dev": "npm run build && NODE_ENV=development nodemon server/index.js --exec babel-node --watch server",
    "box:dev": "nodemon lib/index.js --exec babel-node",
    "clear": "rm -rf dist/box",
    "prebuild": "npm run clear",
    "build": "webpack --progress --hide-modules --config build/webpack.box.config.js",
    "build:gui": "webpack --progress --hide-modules --config build/webpack.gui-server.config.js && prod=1 node build/build.js",
    "box": "pm2 start ./box/gxb-box.js --name gxb-box",
    "gui": "NODE_ENV=production pm2 start ./gui-server/index.js --name gxb-gui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gxchain/gxb-box.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "keywords": [
    "gxchain",
    "box"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gxchain/gxb-box/issues"
  },
  "homepage": "https://github.com/gxchain/gxb-box#readme",
  "dependencies": {
    "archiver": "^2.0.0",
    "async": "^2.5.0",
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "body-parser": "~1.13.2",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.3.0",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "figlet": "^1.2.0",
    "gxbjs": "^1.2.59",
    "gxbjs-ws": "^1.1.17",
    "immutable": "^3.8.1",
    "ipfs-api": "^14.0.4",
    "iview": "^2.0.0-rc.13",
    "jdenticon": "^1.7.2",
    "js-sha256": "^0.7.1",
    "level": "^1.7.0",
    "morgan": "~1.6.1",
    "pm2": "^4.3.0",
    "qs": "^6.5.1",
    "require-ensure": "^1.0.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "superagent": "^3.5.2",
    "vue": "^2.4.4",
    "vue-router": "^2.7.0",
    "vue-timeago": "^3.3.6",
    "vue-websocket": "^0.2.2",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
